{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "chrome": false,
    "saveAs": false
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": "warn",
    "no-param-reassign": "warn"
  }
}