{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "webextensions": true,
    "es6": true
  },
  "globals": {
    "saveAs": false
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": "warn",
    "no-param-reassign": "warn",
    "import/no-unresolved": { "commonjs": true }
  },
  "settings": {
    "import/resolver": "webpack"
  }
}